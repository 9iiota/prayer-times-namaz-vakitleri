{
    "manifest_version": 3,
    "name": "Prayer Times / Namaz Vakitleri",
    "description": "Display prayer times for your location.",
    "version": "0.0.1",
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "storage"
    ],
    "content_scripts": [
		{
			"css": ["styles.css"],
			"js": ["content.js"],
			"matches": ["*://*.namazvakitleri.diyanet.gov.tr/*"]
		}
	]
}